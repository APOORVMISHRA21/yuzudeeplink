<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:title="@string/theme_management">
    <Preference
        android:key="import_theme"
        android:title="@string/theme_import"
        app:iconSpaceReserved="false" />

    <Preference
        android:key="delete_theme"
        android:title="@string/delete_theme"
        app:iconSpaceReserved="false" />

    <jp.hazuki.yuzubrowser.ui.preference.StrToIntListPreference
        android:key="navigation_bar_color_type"
        android:title="@string/pref_navigation_bar_color"
        android:entryValues="@array/pref_navigation_bar_values"
        android:entries="@array/pref_navigation_bar_entries"
        app:iconSpaceReserved="false" />
</PreferenceScreen>
